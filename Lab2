: sqrt-int
    dup 0< if
        drop -1 exit
    then
    
    dup 0= over 1 = or if
        exit
    then
    
    dup >r
    dup
    1
    
    begin
        2dup >
    while
        over over + 2/
        swap drop
        r@ over /
    repeat
    
    nip
    r> drop
;

: main
    25 \ фиксированное значение; 
    sqrt-int . cr
;

main
